{
  "name": "server-ts",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run tsc --project tsconfig.compile.json",
    "build:w": "tsc --project tsconfig.compile.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write"
  },
  "dependencies": {
    "@tsed/ajv": "^6.102.8",
    "@tsed/common": "^6.102.8",
    "@tsed/core": "^6.102.8",
    "@tsed/di": "^6.102.8",
    "@tsed/engines": "^1.1.5",
    "@tsed/exceptions": "^6.102.8",
    "@tsed/json-mapper": "^6.102.8",
    "@tsed/logger": "^6.1.0",
    "@tsed/logger-file": "^6.1.0",
    "@tsed/mongoose": "^6.102.8",
    "@tsed/platform-express": "^6.102.8",
    "@tsed/schema": "^6.102.8",
    "ajv": "^8.10.0",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "method-override": "^3.0.0",
    "mongoose": "^5.12.2"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.17.1",
    "@tsed/cli-plugin-jest": "3.17.1",
    "@tsed/cli-plugin-mongoose": "3.17.1",
    "@tsed/testing-mongoose": "^6.102.8",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "angular",
    "architecture": "default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
